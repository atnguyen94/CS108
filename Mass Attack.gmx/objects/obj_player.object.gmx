    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get player input

key_right = keyboard_check(ord("D"));
key_left = -keyboard_check(ord("A"));
key_jump = keyboard_check_pressed(ord("W")); 
key_down = keyboard_check(ord("S"));

move = key_right + key_left;
hsp = move * movespeed; 


if(vsp &lt; 10) vsp += grav;

// Jumping
if (place_meeting(x, y+1, obj_floor)) || (place_meeting(x, y+1, obj_steps)){
    jumps = max_jumps;  // If we touch the floor or step, reset double jump
}

if(key_jump) &amp;&amp; (jumps &gt; 0) {   
    jumps -= 1; 
    vsp = -jumpspeed;
    
}
// Keeps character within the screen
x=clamp(x,0,room_width-sprite_width);



// Collision with floor
if(place_meeting(x, y+1, obj_floor)){
    if(key_jump){
        vsp = -jumpspeed;
    }
}
if(place_meeting(x, y+1, obj_steps)){
    vsp = key_jump * -jumpspeed;
}



// Step Collision
if(place_meeting(x, y + vsp, obj_steps)){   // Vertical
    while(!place_meeting(x, y + sign(vsp), obj_steps)){
        y += sign(vsp);
    }
    vsp = 0;
}

if(place_meeting(x + hsp, y, obj_steps)){   // Horizontal 
    while(!place_meeting(x + sign(hsp), y, obj_steps)){
        x += sign(hsp);
    }
    hsp = 0;
}

// Floor collision
if(place_meeting(x, y + vsp, obj_floor)){   // Vertical
    while(!place_meeting(x, y + sign(vsp), obj_floor)){
        y += sign(vsp);
    }
    vsp = 0;
}

if(place_meeting(x + hsp, y, obj_floor)){   // Horizontal
    while(!place_meeting(x + sign(hsp), y, obj_floor)){
        x += sign(hsp);
    }
    hsp = 0;
}



// Move the player to these coordinates
x += hsp;
y += vsp;

// Falls to death 

if(y &gt; room_height){
    instance_destroy();
    game_restart();
}


</string>
          </argument>
        </arguments>
      </action>
